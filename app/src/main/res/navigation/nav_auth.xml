<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_mainj"
    app:startDestination="@id/authFragment">

    <fragment
        android:id="@+id/authFragment"
        android:name="com.example.govjobkg.ui.auth.AuthFragment"
        tools:layout="@layout/fragment_auth"
        android:label="AuthFragment" >
        <action
            android:id="@+id/action_authFragment_to_hrMainFragment"
            app:destination="@id/hrMainFragment"

            />
        <action
            android:id="@+id/action_authFragment_to_userMainFragment"
            app:destination="@id/userMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/hrMainFragment"
        android:name="com.example.govjobkg.ui.hr.HrMainFragment"
        tools:layout="@layout/fragment_main_hr"
        android:label="HrMainFragment" />
    <fragment
        android:id="@+id/userMainFragment"
        android:name="com.example.govjobkg.ui.user.main.UserMainFragment"
        android:label="fragment_user_home"
        tools:layout="@layout/fragment_user_main" />
</navigation>