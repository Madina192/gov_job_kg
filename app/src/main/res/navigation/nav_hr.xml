<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_hr"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.govjobkg.ui.hr.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_createNewVacancyFragment"
            app:destination="@id/createNewVacancyFragment" />
    </fragment>
    <fragment
        android:id="@+id/savedFragment"
        android:name="com.example.govjobkg.ui.hr.SavedFragment"
        android:label="SavedFragment" />
    <fragment
        android:id="@+id/messagesFragment"
        android:name="com.example.govjobkg.ui.hr.MessagesFragment"
        android:label="MessagesFragment" />
    <fragment
        android:id="@+id/applicationsFragment"
        android:name="com.example.govjobkg.ui.hr.ApplicationsFragment"
        android:label="ApplicationsFragment" />
    <fragment
        android:id="@+id/createNewVacancyFragment"
        android:name="com.example.govjobkg.ui.hr.CreateNewVacancyFragment"
        android:label="CreateNewVacancyFragment" >
        <action
            android:id="@+id/action_createNewVacancyFragment_to_successFragment"
            app:destination="@id/successFragment" />
    </fragment>
    <fragment
        android:id="@+id/successFragment"
        android:name="com.example.govjobkg.ui.hr.SuccessFragment"
        android:label="SuccessFragment" />
</navigation>